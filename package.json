	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.10",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.1",
		"obsidian": "^1.8.7",
		"obsidian-typings": "^2.35.0",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2"
	}
