	"description": "Switch between Obsidian tabs using your mouse wheel when hovering over tab headers",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
